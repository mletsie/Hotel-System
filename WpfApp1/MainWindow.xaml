<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Login" Height="350" Width="525" MaxHeight="350" MaxWidth="525" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox Name="Username" HorizontalAlignment="Left" Height="23" Margin="183,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="180"/>
        <PasswordBox Name="Password" HorizontalAlignment="Left" Margin="183,108,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Width="180">
            <PasswordBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.146"/>
                    <TranslateTransform/>
                </TransformGroup>
            </PasswordBox.RenderTransform>
        </PasswordBox>
        <Button Content="Login" HorizontalAlignment="Left" Margin="183,150,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Register" HorizontalAlignment="Left" Margin="183,189,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>

    </Grid>
</Window>
